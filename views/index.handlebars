<div class="container">
	<div class="jumbotron">
	  <h1>Simplify your links</h1>

	  <form>
		  <div class="form-group">
		    <input type="text" class="form-control" id="url-name" placeholder="Your original URL here">
		  </div>
		  <button type="submit" class="btn btn-default">SHORTEN URL</button>
		</form>
	</div>

	<div class="panel panel-default">
	  <!-- Default panel contents -->
	  <div class="panel-heading">Panel heading</div>

	  <!-- Table -->
	  <table class="table">
	    <thead>
	    	<th>Original URL</th>
	    	<th>Short URL</th>
	    </thead>
	    <tbody>
	    	{{#each data as |urlpair|}}
	    		<tr>
	    			<td><a href="http://{{urlpair.value}}"> {{urlpair.value}}</a></td>
	    			<td><a href="{{urlpair.key}}"> {{urlpair.key}}</a></td>
	    		</tr>
	    	{{/each}}
	    </tbody>
	  </table>
	</div>
</div>
